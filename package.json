{
  "name": "cinemic",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "dotenvx run -f .env.production -- next start -p 3443",
    "dev": "dotenvx run -f .env.production -- next dev -p 3443",
    "dev:https": "dotenvx run -f .env.production -- next dev --experimental-https -p 3443",
    "prod": "dotenvx run -f .env.production -- next build",
    "prod:analyze": "dotenvx run -f .env.production --overload --env ANALYZE=true -- next build --webpack",
    "server": "dotenvx run -f .env -- nodemon --watch src/server --ext js,ts,json --exec \"tsx --tsconfig tsconfig.api.json src/server/app/entrypoint/index.ts\"",
    "test": "dotenvx run -f .env.production -- jest",
    "type-check": "tsc --noEmit",
    "dep:cycles:client": "madge --extensions ts,tsx,js --ts-config tsconfig.json --circular src/client",
    "dep:cycles:server": "madge --extensions ts,tsx,js --ts-config tsconfig.api.json --circular src/server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
