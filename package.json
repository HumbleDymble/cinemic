{
  "name": "cinemic",
  "version": "1.0.2",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=tsconfig.node.json NODE_OPTIONS=\"--import tsx\" webpack serve --config webpack.config.ts --config-name client --env mode=development",
    "dev": "cross-env TSX_TSCONFIG_PATH=tsconfig.node.json NODE_OPTIONS=\"--import tsx\" webpack --config webpack.config.ts --config-name client --env mode=development",
    "prod": "cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.node.json NODE_OPTIONS=\"--import tsx\" webpack --config webpack.config.ts --config-name client --env mode=production",
    "prod:analyze": "cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.node.json NODE_OPTIONS=\"--import tsx\" webpack --config webpack.config.ts --config-name client --env mode=production --env analyzer=true",
    "type-check": "tsc --noEmit",
    "server": "dotenvx run -f .env -- nodemon --watch src/server --ext js,ts,json --exec \"tsx --tsconfig tsconfig.node.json src/server/app/entrypoint/index.ts\"",
    "dep:cycles:client": "madge --extensions ts,tsx,js --ts-config tsconfig.json --circular src/client",
    "dep:cycles:server": "madge --extensions ts,js --ts-config tsconfig.node.json --circular src/server",
    "test": "dotenvx run -f .env.production -- jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
